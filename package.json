{
  "name": "@seamonster-studios/rescript-index",
  "version": "0.0.0",
  "author": "SeaMonster Studios Engineering",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "make": "node ./bin/make-package",
    "docs:dev:cosmos": "cosmos",
    "docs:dev:snowpack": "snowpack dev",
    "docs": "yarn docs:build:cosmos && run-p docs:dev:snowpack docs:dev:cosmos",
    "docs:build:snowpack": "snowpack build",
    "docs:build:cosmos": "cosmos-export",
    "docs:serve:cosmos": "npx serve cosmos-export",
    "docs:build": "run-s docs:build:snowpack docs:build:cosmos",
    "docs:export": "run-s docs:build:snowpack docs:serve:cosmos",
    "bootstrap": "./node_modules/.bin/lerna bootstrap",
    "deploy": "./node_modules/.bin/lerna publish",
    "clean": "rm -rf ./build ./cosmos-export ./packages/cosmos.userdeps.js"
  },
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "rescript"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-postcss": "^1.2.2",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "autoprefixer": "^10.2.5",
    "change-case": "^4.1.2",
    "fs-extra": "^9.1.0",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.1",
    "react-cosmos": "^5.6.2",
    "react-dom": "^17.0.1",
    "serve": "^11.3.2",
    "snowpack": "^3.0.13",
    "tailwindcss": "^2.1.1"
  },
  "dependencies": {}
}
