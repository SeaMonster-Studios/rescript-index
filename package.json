{
  "name": "@seamonster-studios/rescript-index",
  "version": "0.0.0",
  "author": "SeaMonster Studios Engineering",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "make": "node ./bin/make-package",
    "docs:dev:cosmos": "cosmos",
    "docs:dev:snowpack": "snowpack dev",
    "docs": "run-p docs:dev:snowpack docs:dev:cosmos",
    "docs:build:snowpack": "snowpack build",
    "docs:build:cosmos": "cosmos-export",
    "docs:build": "run-p docs:build:snowpack docs:build:cosmos",
    "serve:docs": "npx serve cosmos-export",
    "build:docs": "run-s build:snowpack build:cosmos serve:docs",
    "bootstrap": "./node_modules/.bin/lerna bootstrap",
    "deploy": "./node_modules/.bin/lerna publish"
  },
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "rescript"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "change-case": "^4.1.2",
    "fs-extra": "^9.1.0",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-cosmos": "^5.6.0",
    "react-dom": "^17.0.1",
    "serve": "^11.3.2",
    "snowpack": "^3.0.13"
  },
  "dependencies": {}
}
